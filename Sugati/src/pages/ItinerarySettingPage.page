            $scope.loading = function(action){
                if(action == "show"){
                    $('#contentLoading').css("display","block");
                }
                else{
                    $('#contentLoading').css("display","none");
                }
            }
            //Refresh Map
            $scope.refreshMap = function(country,selectedValue){
                $scope.MapTravellingCountry[country]['{!nameSpacePrefix}TC_Accommodation_Region__c'] = selectedValue;  
            }
            //Google Map
            $scope.getStaticMap = function(type,country) {
              var type = $scope.opportunity['{!nameSpacePrefix}O_Map_Type__c'];
              var markerString = ''; 
              if(type == 'Single'){
                  angular.forEach($scope.MapTravellingCountry,function(value,index){
                      if(value['{!nameSpacePrefix}TC_Accommodation_Region__c'] =='Region'){
                          angular.forEach($scope.MapCountryToWTGList[value['{!nameSpacePrefix}TC_Country__r'].Name],function(wtg,index){
                              markerString += '&markers='+encodeURIComponent('color:orange|'+'label:'+String.fromCharCode(index+65)+'|'+wtg.replace(/##/g,','))+'+'+value['{!nameSpacePrefix}TC_Country__r'].Name;    
                          });
                      }
                      else{
                          angular.forEach($scope.mapCountryToAccom[value['{!nameSpacePrefix}TC_Country__r'].Name],function(wtg,index){
                              markerString += '&markers='+encodeURIComponent('icon:http://chart.apis.google.com/chart?chst=d_map_pin_icon&chld=cafe|color:blue|'+'label:'+String.fromCharCode(index+65)+'|'+wtg.replace(/##/g,','))+'+'+value['{!nameSpacePrefix}TC_Country__r'].Name;    
                          });
                      }
                  });
              }  
              else{
                  angular.forEach($scope.MapTravellingCountry,function(value,index){
                      if(value['{!nameSpacePrefix}TC_Accommodation_Region__c'] =='Region'){
                          angular.forEach($scope.MapCountryToWTGList[country],function(wtg,index){
                              markerString += '&markers='+encodeURIComponent('color:orange|'+'label:'+String.fromCharCode(index+65)+'|'+wtg.replace(/##/g,','))+'+'+value['{!nameSpacePrefix}TC_Country__r'].Name;    
                          });
                      }
                      else{
                          angular.forEach($scope.mapCountryToAccom[country],function(wtg,index){
                              markerString += '&markers='+encodeURIComponent('icon:http://chart.apis.google.com/chart?chst=d_map_pin_icon&chld=cafe|color:blue|'+'label:'+String.fromCharCode(index+65)+'|'+wtg.replace(/##/g,','))+'+'+value['{!nameSpacePrefix}TC_Country__r'].Name;    
                          });
                      }
                  });    
              }
              var src = "http://maps.googleapis.com/maps/api/staticmap?size=550x450&maptype=roadmap"+markerString;
              return src;
            }
            
            
            angular.forEach($scope.supplierBookings,function(value,index){
                if($scope.Dates.indexOf(value['{!nameSpacePrefix}SB_From_Date__c']+'-'+value['{!nameSpacePrefix}SB_To_Date__c']) == -1){
