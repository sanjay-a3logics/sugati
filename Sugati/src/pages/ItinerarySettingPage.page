                   if(value.tc['{!nameSpacePrefix}TC_DestinationMap_Image_ID__c'] === undefined){
                       value.tc['{!nameSpacePrefix}TC_DestinationMap_Image_ID__c'] = null;    
                   }
               });
               if($scope.opportunity['{!nameSpacePrefix}O_Destination_Map_Image_ID__c'] === undefined){
                   $scope.opportunity['{!nameSpacePrefix}O_Destination_Map_Image_ID__c'] = null;
               }
               console.log($scope.imagesForSave);
               $scope.loading("show")
