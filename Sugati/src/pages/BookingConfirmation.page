                        .html('Please select Insurance checkbox')
                        .dialog({
                            modal: true,
                            title: 'Error',
                            zIndex: 10000,
                            autoOpen: true,
                            width: '415px',
                            resizable: false,
                            buttons: {
                                OK: function () {
                                    $(this).dialog("close");
                                }
                            },
