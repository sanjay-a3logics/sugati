                                $(this).remove();
                            }
                        });
                        return false;
                    }
                }
                else{
                
                }
            }
            
            function ValidateConfirm(Oppid){
                var count = 0;
                console.log('Validate -- ');
                $('.chk').each(function(){
                    if($(this).attr('checked')) {
                        count = 1;
                    }
                });
                calculateTotal();
                console.log('Validate -- '+count);
                var isNameValid = true;
                var isAmountValid = true;
                var isCardNumberValid = true;
                var isExpirayDateValid = true;
                var isCVVValid = true;
                var isCountrySelected = true;
                $('.card-name, card-number, .card-amount, .card-expiry, .card-code, .card-country').removeClass('has-error');
                   
                if($('.card-details').length != 0){
                    if($('.card-name-field').val() == ''){
                        isNameValid = false;
                        $('.card-name').addClass('has-error');
                    }
                    if($('.card-amount-field').val() <= 0){
                        isAmountValid = false;
                        $('.card-amount').addClass('has-error');
                    }
                    if($('.card-number-field').val().trim() == '' || $('.card-number-field').val().length<12){
                        isCardNumberValid = false;
                        $('.card-number').addClass('has-error');
                    }
                    if($('.card-expiry-field:first').val() == '' || $('.card-expiry-field:last').val() ==''){
                        isExpirayDateValid = false;
                        $('.card-expiry').addClass('has-error');
                    }
                    if($('.card-country-field').val() ==''){
                        isCountrySelected = false;
                        $('.card-country').addClass('has-error');
                    } 
                    if($('.card-code-field').val().trim() ==''){
                        isCountrySelected = false;
                        $('.card-code').addClass('has-error');
                    }     
                }
                var value = parseFloat($('.card-amount-field').val());
                if( value != 0 && value > 0 && count == 1 && isNameValid && isAmountValid && isCardNumberValid && isExpirayDateValid && isCVVValid && isCountrySelected){
                    Confirm();
                }
                else if(!(value == 0 || value < 0)){
                    if(!(isNameValid && isAmountValid && isCardNumberValid && isExpirayDateValid && isCVVValid && isCountrySelected)){
                        alert('Please provide valid card detais');  
                        var errorDiv = $('#errorMsg');
                        var scrollPos = errorDiv.offset().top;
                       // $(window).scrollTop(scrollPos);   
                    }
                    else{
                        alert('Please select a payment option first.');   
                    }
                    
                }
                else if(value == 0 || value < 0){
                    alert('please enter an amount greater than 0.');    
                }
            }
            
            function alert(value){
                $('<div></div>').appendTo('body')
                .html(value)
                .dialog({
                    modal: true,
                    title: 'Error',
                    zIndex: 10000,
                    autoOpen: true,
                    width: '415px',
                    resizable: false,
                    buttons: {
                        OK: function () {
                            $(this).dialog("close");
                        }
                    },
                    close: function (event, ui) {
                        $(this).remove();
                    }
                }); 
            }
            function loading(val){
                if (val) {
                    document.getElementById('contentLoading').style.display = 'inline-block';
                    document.getElementById('contentLoaded').style.display = 'none';
                }
                else {
                    document.getElementById('contentLoading').style.display = 'none';
                    document.getElementById('contentLoaded').style.display = 'inline-block';
                }
            }
            
            function calculateTotal(type){
                if(type){
                    var amount = document.getElementById('j_id0:frm:dueAmount').value;
                    var cc = document.getElementById('ccCharges').value;
                    var bookingAmount = parseFloat(amount);
                    var creditCardCharges = ((parseFloat(amount)*cc)/100);
                    if(cc == 0){
                        creditCardCharges = 0;
                    }
                    var amountToBePaid = (bookingAmount + creditCardCharges).toFixed(2);
                    document.getElementById('totalPayment').value = amountToBePaid;
                    document.querySelector('.totalPayment').value = amountToBePaid;
                }
            }
            
            
        </script>
